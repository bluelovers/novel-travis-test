{
  "name": "novel-travis-test",
  "version": "1.0.0",
  "private": true,
  "description": "<%= description %>",
  "keywords": [],
  "homepage": "https://github.com/bluelovers/novel-travis-test#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/novel-travis-test/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/novel-travis-test.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "bin": {
    "novel-travis-test": "bin/_run-task.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "node -v",
    "npm:publish": "npm publish",
    "npm:publish:lerna": "ynpx --quiet lerna -- publish --yes --bump patch",
    "prepublishOnly_": "yarn run prepublishOnly:check-bin && yarn run prepublishOnly:update && yarn run test",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "postpublish": "yarn run postpublish:changelog && yarn run postpublish:git:commit && yarn run postpublish:git:tag && yarn run postpublish:git:push",
    "postpublish:changelog": "ynpx --quiet @yarn-tool/changelog && git add ./CHANGELOG.md",
    "postpublish:git:commit": "git commit -m \"chore(release): publish\n\n[skip ci]\" . & echo postpublish:git:commit",
    "postpublish:git:push": "git push --follow-tags",
    "postpublish:git:tag": "ynpx --quiet @yarn-tool/tag -- --force-git-tag",
    "do-bugfix": "node ./bin/_bugfix.js",
    "do-epub": "node ./bin/_run-epub.js",
    "do-epub-init": "node ./bin/_do_epub_init.js",
    "do-epub-push": "node ./bin/_do_epub_push.js",
    "do-git-init": "node ./bin/_do_git_init.js",
    "do-task": "node ./bin/_run-task.js",
    "do-toc": "node ./bin/_create_toc.js",
    "do-toc-contents": "node ./bin/_create_toc_contents.js",
    "list:all:cache": "node ./script/cache/print.js",
    "ncu": "ynpx --quiet yarn-tool -- ncu -u",
    "output-stat": "node ./bin/_output_stat.js",
    "reset-epub-init-cahche": "node ./script/reset/reset-epub-init-cahche.js",
    "reset-init-cahche": "node ./script/reset/reset-init-cahche.js",
    "set-cache": "node ./bin/_set_cache.js",
    "sort-package-json": "ynpx --quiet yarn-tool -- sort",
    "update-meta-cache": "node ./bin/_update_meta.js"
  },
  "dependencies": {
    "@bluelovers/fast-glob": "^3.0.4",
    "@node-novel/cache-loader": "^1.0.34",
    "@node-novel/md-loader": "^1.0.12",
    "@node-novel/sort": "^1.0.17",
    "@node-novel/task": "^1.0.30",
    "@node-novel/toc": "^1.0.58",
    "array-hyper-unique": "^1.1.9",
    "bluebird": "^3.7.2",
    "chai": "^4.3.0",
    "cjk-conv": "^1.2.123",
    "client-oauth2-request": "^1.0.0",
    "crlf-normalize": "^1.0.5",
    "cross-spawn-extra": "^2.1.0",
    "debug-color2": "^1.2.3",
    "dotenv": "^8.2.0",
    "email-normalize": "^1.0.0",
    "env-bool": "^1.0.3",
    "fs-extra": "^9.1.0",
    "fs-iconv": "^3.0.12",
    "git-diff-from": "^1.1.21",
    "git-root2": "^1.0.22",
    "gitee-api-token": "^1.0.1",
    "gitlab": "< 5 & >= 4",
    "gitlog2": "^5.0.17",
    "moment": "^2.29.1",
    "node-novel-info": "^3.0.13",
    "novel-epub": "^2.0.5",
    "novel-segment": "^2.7.57",
    "novel-txt-merge": "^1.0.29",
    "prettyuse": "^0.0.0",
    "segment-dict": "^2.3.139",
    "sort-object-keys2": "^2.0.3",
    "source-map-support": "^0.5.19",
    "uni-string": "^1.1.10",
    "upath2": "^3.1.7",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.20",
    "@types/bluebird": "^3.5.33",
    "@types/chai": "^4.2.15",
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^9.0.7",
    "@types/node": "^14.14.26",
    "@types/source-map-support": "^0.5.3",
    "@types/yargs": "^16.0.0",
    "travis-ci-encrypt": "^1.0.7"
  }
}
