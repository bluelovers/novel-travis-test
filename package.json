{
  "name": "novel-travis-test",
  "version": "1.0.0",
  "private": true,
  "description": "<%= description %>",
  "homepage": "https://github.com/bluelovers/novel-travis-test#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/novel-travis-test/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/novel-travis-test.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "bin": {
    "novel-travis-test": "bin/_run-task.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "node -v",
    "npm:publish": "npm publish",
    "npm:publish:lerna": "npx lerna publish --yes --cd-version patch",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "do-bugfix": "node ./bin/_bugfix.js",
    "do-epub": "node ./bin/_run-epub.js",
    "do-epub-init": "node ./bin/_do_epub_init.js",
    "do-epub-push": "node ./bin/_do_epub_push.js",
    "do-git-init": "node ./bin/_do_git_init.js",
    "do-task": "node ./bin/_run-task.js",
    "do-toc": "node ./bin/_create_toc.js",
    "do-toc-contents": "node ./bin/_create_toc_contents.js",
    "list:all:cache": "node ./script/cache/print.js",
    "ncu": "npx yarn-tool ncu -u",
    "output-stat": "node ./bin/_output_stat.js",
    "reset-epub-init-cahche": "node ./script/reset/reset-epub-init-cahche.js",
    "reset-init-cahche": "node ./script/reset/reset-init-cahche.js",
    "set-cache": "node ./bin/_set_cache.js",
    "sort-package-json": "npx yarn-tool sort",
    "update-meta-cache": "node ./bin/_update_meta.js"
  },
  "dependencies": {
    "@bluelovers/tsconfig": "^1.0.19",
    "@node-novel/cache-loader": "^1.0.15",
    "@node-novel/md-loader": "^1.0.5",
    "@node-novel/sort": "^1.0.8",
    "@node-novel/task": "^1.0.29",
    "@node-novel/toc": "^1.0.42",
    "@types/dotenv": "^8.2.0",
    "array-hyper-unique": "^1.1.0",
    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "cjk-conv": "^1.2.44",
    "client-oauth2-request": "^1.0.0",
    "crlf-normalize": "^1.0.3",
    "cross-spawn-extra": "^2.0.0",
    "debug-color2": "^1.1.7",
    "dotenv": "^8.2.0",
    "email-normalize": "^1.0.0",
    "env-bool": "^1.0.3",
    "fast-glob": "^3.2.2",
    "fs-extra": "^9.0.0",
    "fs-iconv": "^3.0.4",
    "git-diff-from": "^1.1.5",
    "git-root2": "^1.0.7",
    "gitee-api-token": "^1.0.1",
    "gitlab": "< 5 & >= 4",
    "gitlog2": "^5.0.3",
    "moment": "^2.26.0",
    "node-novel-info": "^3.0.1",
    "novel-epub": "^1.2.4",
    "novel-segment": "^2.5.16",
    "novel-txt-merge": "^1.0.24",
    "prettyuse": "^0",
    "sort-object-keys2": "^2.0.3",
    "source-map-support": "^0.5.19",
    "uni-string": "^1.1.2",
    "upath2": "^2.1.3",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.31",
    "@types/chai": "^4.2.11",
    "@types/fs-extra": "^9.0.1",
    "@types/source-map-support": "^0.5.1",
    "@types/yargs": "^15.0.5",
    "travis-ci-encrypt": "^1.0.7"
  }
}
